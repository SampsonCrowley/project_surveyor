<% content_for :sidenav do %>
  <%= render partial: 'admin/shared/builder_nav' %>
  <div class="col-sm-10">
<% end %>
<div class="col-xs-12">
  <section class="panel panel-default">

    <div class="panel-heading">
      <h2 class="panel-title">Survey Builder Homepage</h2>
    </div>

    <!-- Default panel contents -->
    <div class="panel-body">
      <% @surveys.each do |survey| %>
        <%= render partial: 'survey_link', locals: { survey: survey } %>
      <% end %>
    </div>

    <div class="panel-footer">
    </div>
  </section>
</div>
