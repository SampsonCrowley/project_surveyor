<% content_for :sidenav do %>
  <%= render partial: 'admin/shared/builder_nav' %>
  <div class="col-sm-10">
<% end %>
<div class="col-xs-12">
  <section class="panel panel-default">

    <div class="panel-heading">
      <h2 class="panel-title">Create New Survey</h2>
    </div>

    <!-- Default panel contents -->
    <div class="panel-body">
      <%= render partial: 'admin/shared/survey_info', locals: { survey: @survey } %>
      <%= render_partial_for_category(@question, @survey, @set_text, @options_count) %>
      <%= render partial: 'admin/shared/existing_questions', locals: { survey: @survey } %>
    </div>

    <div class="panel-footer">
    </div>
  </section>
</div>
